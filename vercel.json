{
    "builds": [{
        "src": "littlelemon/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLamdaSize": "15mb",
            "runtime": "python3.9",
            "distDir": "staticfiles"
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "littlelemon/wsgi.py"
    }]
}