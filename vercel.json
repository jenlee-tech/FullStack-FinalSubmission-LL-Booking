{
    "version": 2,
    "builds": [{
        "src": "littlelemon/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLamdaSize": "15mb",
            "runtime": "python3.9",
            "includeFiles": ["staticfiles/**", "static/**'"]
        }
    }],
    "routes": [{
            "src": "/(.*)",
            "dest": "littlelemon/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "manage.py"
        }
    ]
}